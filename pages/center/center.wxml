            <view class="info">
                <view class="nickname">
                    用户名
                </view>
                <view class="membershipLevel">
                    <text class="memberTitle1">身份</text>
                </view>
                <view class="icon ion-ios-arrow-right top_right"></view>
            </view>
        </view>
    </view>
    <view class="m-uc-wrap">
        <view class="m-uc-wraptit">
            <text class="txt">我的订单</text>
